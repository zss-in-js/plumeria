{
  "jsPlugins": ["./oxlint-plugin.js"],
  "plugins": [],
  "ignorePatterns": ["**/*.svelte", "**/*.vue"],

  "rules": {
    "@plumeria/no-destructure": "error",
    "@plumeria/no-inner-call": "error",
    "@plumeria/no-unused-keys": "warn",
