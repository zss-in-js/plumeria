{
  "name": "@plumeria/core",
  "version": "3.0.1",
  "description": "An atomic CSS runtime designed to disappear.",
  "author": "Refirst 11",
  "license": "MIT",
  "funding": "https://github.com/sponsors/refirst11",
  "homepage": "https://plumeria.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zss-in-js/plumeria.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/zss-in-js/plumeria/issues"
  },
  "keywords": [
    "plumeria",
    "atomic-css",
    "css-in-js",
    "css",
    "stylex",
    "react-native-web"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./stylesheet.css": "./stylesheet.css",
    ".": {
      "types": "./dist/css.d.ts",
      "import": "./dist/css.mjs",
      "default": "./dist/css.js"
    }
  },
  "main": "dist/css.js",
  "module": "dist/css.mjs",
  "types": "dist/css.d.ts",
  "files": [
    "dist/",
    "stylesheet.css"
  ],
  "scripts": {
    "build": "tsdown && node strip.ts"
  },
  "dependencies": {
    "csstype": "^3.2.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
