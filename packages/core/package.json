{
  "name": "@plumeria/core",
  "version": "0.20.0",
  "description": "The atomic on-demand CSS-in-JS.",
  "author": "Refirst 11",
  "license": "MIT",
  "funding": "https://github.com/sponsors/refirst11",
  "homepage": "https://plumeria.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zss-in-js/plumeria.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/zss-in-js/plumeria/issues"
  },
  "keywords": [
    "plumeria",
    "atomic-css",
    "css-in-js",
    "css",
    "stylex",
    "react-native-web"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./stylesheet.css": "./stylesheet.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./processors": {
      "types": "./dist/processors/css.d.ts",
      "import": "./dist/processors/css.mjs",
      "default": "./dist/processors/css.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "stylesheet.css"
  ],
  "scripts": {
    "build": "tsdown && node strip-comments"
  },
  "dependencies": {
    "zss-engine": "0.2.90"
  },
  "publishConfig": {
    "access": "public"
  }
}
