{
  "name": "@plumeria/core",
  "version": "0.9.1",
  "description": "Zero-runtime CSS in JS library in TypeScript.",
  "keywords": [
    "css",
    "css-in-js",
    "plumeria",
    "react",
    "next",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zss-in-js/plumeria.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./stylesheet.css": "./stylesheet.css",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./processors": {
      "types": "./types/processors/css.d.ts",
      "import": "./dist/esm/processors/css.js",
      "default": "./dist/cjs/processors/css.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/",
    "types/",
    "stylesheet.css"
  ],
  "scripts": {
    "build": "rimraf dist types && pnpm esm && pnpm cjs",
    "cjs": "tsc --project tsconfig.cjs.json",
    "esm": "tsc --project tsconfig.esm.json"
  },
  "dependencies": {
    "zss-engine": "0.2.32",
    "zss-utils": "0.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
