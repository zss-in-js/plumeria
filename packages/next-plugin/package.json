{
  "name": "@plumeria/next-plugin",
  "version": "0.29.0",
  "description": "Plumeria Next.js plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zss-in-js/plumeria.git",
    "directory": "packages/next-plugin"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./turbopack": {
      "types": "./dist/turbopack.d.ts",
      "import": "./dist/turbopack.js",
      "default": "./dist/turbopack.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rimraf dist && pnpm cjs",
    "cjs": "tsc --project tsconfig.cjs.json"
  },
  "dependencies": {
    "@plumeria/webpack-plugin": "workspace:^",
    "@plumeria/turbopack-loader": "workspace:^"
  },
  "devDependencies": {
    "next": "^16.0.3",
    "webpack": "^5.101.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
