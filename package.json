    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.10",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "tsdown": "^0.11.11",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.26.1"
  },
  "packageManager": "pnpm@10.11.0"
